luminosity=19790.
mkdir eleTauPlots

MakeDataCard channel='eleTau'    dataFile='../inputs/ltau/DATA.root' zEmbeddedSample='../inputs/ltau/ETEM.root'  wThreeJetsFile='../inputs/ltau/WJETS.root' zttFile='../inputs/ltau/ZJETS.root' zllFile='../inputs/ltau/ZJETS.root' wFile='../inputs/ltau/WJETS.root' vvFile='../inputs/ltau/VV.root' topFile='../inputs/ltau/TOP.root' preselection=$preselection       wselection=$wselection	       signalselection=$signalselection         qcdselection=$qcdselection  luminosityErr=0.045 qcdfactor=1.06 qcdfactorErr=0.02 vvErr=0.3   zttScale=1.0 zttScaleErr=0.033    wFactorErr=0.06    zjfterr=0.1  zlfterr=0.24   topErr=0.075 	muID=1.00 muIDErr=0.01 eleID=0.0 eleIDErr=0.0 tauID=1.0 tauIDErr=0.06  dir='../inputs/ltau/'  bId=1.0  bIDErr=0.10  bMisID=1.0  bMisIDErr=0.17    bitmask=1,0,0,0  vbfSelection0=$vbfSelection0 vbfSelection1=$vbfSelection1  vbfSelection2=$vbfSelection2   relaxedSelection=$relaxedSelection  weight='__WEIGHT__*__CORR__'  embWeight=$embWeight  luminosity=$luminosity  variable='pt2preES' bins=30 min=0.0 max=120 outputFile='eleTauPlots/eleTau_pt2NOES.root'

