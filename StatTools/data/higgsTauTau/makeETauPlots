#!/bin/sh


preselection='pt1>24&&pt2preES>20&&vertices>0&&diLeptons==0&&lPFIsoDB<0.1&&tauIso<1.5&&tightMuons==0&&tightElectrons<=1'
wselection='mt1>70'
signalselection='mt1<30'
qcdselection='pt1>24&&pt2>20&&vertices>0&&diLeptons==0&&lPFIsoDB>0.2&&tauIso<1.5&&charge!=0&&mt1<30&&lTrigger>0&&tauTrigger>0'
